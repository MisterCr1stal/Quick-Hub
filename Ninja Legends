        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("Quick Hub - Ninja Legends", "BloodTheme")



        local Tab = Window:NewTab("Credits")
    local Section = Tab:NewSection("Credits")
    local Section = Tab:NewSection("Made by: RealLeon#0005")
    local Section = Tab:NewSection("Helper: NezyGhoul#8130")
    local Section = Tab:NewSection("https://discord.gg/vAzFEbfs")
    

  
        
        local Tab = Window:NewTab("AutoFarm")
        local Section = Tab:NewSection("AutoFarm")
        
        Section:NewToggle("Auto Rank", "New AutoRank", function(state)
            if state then
                print("Toggle On")
                _G.sword = true
while _G.sword do
wait()
--This script was generated by Hydroxide
local oh1 = "buyRank"
local oh2 = game:GetService("ReplicatedStorage").Ranks.Ground:GetChildren()
for i = 1,#oh2 do
game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i].Name)
end
end
            else
                print("Toggle Off")
                _G.sword = true
while _G.sword do
wait()
local oh1 = "buyRank"
local oh2 = game:GetService("ReplicatedStorage").Ranks.Ground:GetChildren()
for i = 1,#oh2 do
game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(oh1, oh2[i].Name)
end
end
            end
        end)
        
        
        
        
        
        
        
        
        
        
        
        
        
        
